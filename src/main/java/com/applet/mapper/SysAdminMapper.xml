<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.applet.mapper.SysAdminMapper">
    <insert id="createAdmin" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO sys_access(`name`,path) VALUES(#{name},#{path})
        ON DUPLICATE KEY UPDATE `name`=#{name},path=#{path}
    </insert>

    <delete id="deleteAdmin">
        DELETE FROM sys_access WHERE id=#{id}
    </delete>

    <update id="updateAdmin">
        UPDATE sys_access SET `name`=#{name},path=#{path} WHERE id=#{id}
    </update>

    <select id="getAdmin" resultType="SysAdmin">
        select * from sys_access
    </select>
</mapper>